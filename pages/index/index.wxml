<view class="shop_index">
    <!--搜索框开始-->
    <SearchInput></SearchInput>
    <!--搜索框结束-->
    <!-- 轮播图开始 -->
    <view class="index_swiper">
        <swiper autoplay indicator-dots circular>
            <swiper-item wx:for="{{swiperList}}" wx:key="id">
                <navigator>
                    <image mode="widthFix" src="{{item.picUrl}}"></image>
                </navigator>
            </swiper-item>

        </swiper>
    </view>
    <!-- 轮播图结束 -->
</view>